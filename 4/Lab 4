using System;

class Program4
{
    static int Fibonacci(int n)
    {
        if (n == 0)
            return 0;
        else if (n == 1)
            return 1;
        else
            return Fibonacci(n - 1) + Fibonacci(n - 2);
    }

    static void Main()
    {
        int a = 4;
        int result = Fibonacci(a);
        Console.WriteLine(result);
    }
}
