using System;

class Program3 {
    static void divide(ref int a, out int result)
    {
        a = a + 2;
        result = a / 2;
    }

    static void Main()
    {
        int a = 5;
        int result = 0 ;
        divide(ref a,out result);
        Console.WriteLine(a);
        Console.WriteLine(result);
    }
}


