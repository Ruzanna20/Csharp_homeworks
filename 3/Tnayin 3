using System;

class Program7
{
    static void Main(string[] args)
    {
        int n = int.Parse(Console.ReadLine());

        switch (n)
        {
            case 1:
                Console.WriteLine("Հունվար");
                break;
            case 2:
                Console.WriteLine("Փետրվար");
                break;
            case 3:
                Console.WriteLine("Մարտ");
                break;
            case 4:
                Console.WriteLine("Ապրիլ");
                break;
            case 5:
                Console.WriteLine("Մայիս");
                break;
            case 6:
                Console.WriteLine("Հունիս");
                break;
            case 7:
                Console.WriteLine("Հուլիս");
                break;
            case 8:
                Console.WriteLine("Օգոստոս");
                break;
            case 9:
                Console.WriteLine("Սեպտեմբեր");
                break;
            case 10:
                Console.WriteLine("Հոկտեմբեր");
                break;
            case 11:
                Console.WriteLine("Նոյեմբեր");
                break;
            case 12:
                Console.WriteLine("Դեկտեմբեր");
                break;
            default:
                Console.WriteLine("Նման ամիս չկա։");
                break;
        }
    }
}
