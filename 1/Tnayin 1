using System;

class Program4
{
    static void Main()
    {
        Console.WriteLine("Ի՞նչ է քո անունը։");
        string name = Console.ReadLine();

        Console.WriteLine("Ի՞նչ է քո ազգանունը։");
        string surname = Console.ReadLine();

        Console.WriteLine("Ո՞րտեղ ես ապրում։");
        string country = Console.ReadLine();

        Console.WriteLine($"Ես {name} {surname} եմ, ապրում եմ {country}-ում։");
    }
}
